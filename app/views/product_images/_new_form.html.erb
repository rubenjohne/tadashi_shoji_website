<%= form_for(:product_image, :url => collection_style_product_images_path) do |f| %>
<table>
	<tr>
		<th>File :</th>
		<td><%= f.file_field :image_filename %></td>
	</tr>	
	<tr>
		<th>Label :</th>
		<td><%= f.text_field :label %></td>
	</tr>	
	<tr>
		<td><%= f.hidden_field :image_type, :value => image_assigned_type %></td>
		<td><%= submit_tag  %></td>
	</tr>	
</table>
<% end %>